{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "serial-port-tool",
  "version": "0.1.0",
  "identifier": "uefi.yanggang",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "A Serial Port Tool",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDY5Qzg2ODIzMzAzMzYxMTQKUldRVVlUTXdJMmpJYWN4Z3doYjFCQUFQTHhmOWNKYS9DUXVxUjdueE1maExiaWE1WTF3cG4rWWQK",
      "endpoints": [
        "https://github.com/YangGangUEFI/serial-port-tool/releases/latest/download/latest.json"
      ]
    }
  }
}
